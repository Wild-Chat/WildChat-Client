		"@types/wicg-file-system-access": "^2020.9.6",
		"sass": "^1.60.0",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.2.0"
	},
	"type": "module",
	"dependencies": {
		"@fortawesome/free-regular-svg-icons": "^6.4.0",
		"@fortawesome/free-solid-svg-icons": "^6.4.0",
		"@picmo/popup-picker": "^5.8.2",
		"@picmo/renderer-twemoji": "^5.8.2",
		"bootstrap": "^5.2.3",
		"emoji-picker-element": "^1.16.0",
